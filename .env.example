# QCentroid Configuration for DBS Challenge
# Copy this file to .env and fill in your actual values
# DO NOT commit .env to git!

# QCentroid API Credentials (REQUIRED)
QCENTROID_API_KEY=your_api_key_here
QCENTROID_WORKSPACE_ID=your_workspace_id_here

# DBS Challenge API Endpoint
QCENTROID_CHALLENGE_API=https://api.dev.qcentroid.xyz/use-cases/challenge-2-singapore-t4kmam

# Quantum Backend Configuration
QCENTROID_BACKEND=qcentroid  # Using QCentroid platform (not simulator)
QCENTROID_MAX_QUBITS=10      # Maximum qubits to use
QCENTROID_SHOTS=1024         # Number of circuit executions

# Python Environment
PYTHON_ENV=development  # Options: development, production
DEBUG=True

# Model Configuration
MODEL_DIR=model/saved_models
DATA_DIR=data/processed
